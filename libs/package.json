{
  "name": "@newsolution/api-worker",
  "version": "0.0.1",
  "description": "NestJS",
  "author": "newsolutiondev",
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "files": [
    "dist/**/*",
    "*.md"
  ],
  "scripts": {
    "start:dev": "tsc -w",
    "build": "tsc",
    "prepare": "yarn build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "keywords": [
    "nestjs"
  ],
  "publishConfig": {
    "access": "restricted"
  },
  "peerDependencies": {},
  "dependencies": {
    "@newsolution/api-common": "^0.0.48",
    "class-validator": "^0.12.2"
  },
  "devDependencies": {
    "prettier": "2.0.5",
    "ts-node": "8.1.0",
    "tsc-watch": "2.2.1",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.16.0",
    "typescript": "3.7.4"
  }
}
